{
  "name": "@parlai/cli",
  "version": "0.1.0",
  "description": "CLI tool for Parlay - Extract and manage i18n translations in React apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "parlay": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
