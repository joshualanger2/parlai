  },
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "translation",
    "cli",
    "typescript"
  ],
  "author": "Parlay Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/parlai.git"
  },
  "dependencies": {
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "commander": "^12.0.0",
    "glob": "^8.1.0",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@babel/types": "^7.24.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.20.5",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.11.24",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
