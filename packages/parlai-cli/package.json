{
  "name": "parlai",
  "version": "0.1.1",
  "description": "CLI tool to automate i18n setup and management in React applications",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "parlai": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "translation",
    "cli",
    "typescript",
    "next.js"
  ],
  "author": "Parlai Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshualanger2/parlai"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/generator": "^7.24.0",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@types/inquirer": "^9.0.8",
    "commander": "^12.0.0",
    "glob": "^8.1.0",
    "inquirer": "^12.6.1",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__parser": "^7.1.1",
    "@types/babel__traverse": "^7.20.5",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
